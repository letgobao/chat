model members {
  memberId       String @map("member_id") @db.VarChar(35)
  conversationId String @map("conversation_id") @db.VarChar(35)

  conversation conversations @relation(fields: [conversationId], references: [id], onDelete: Cascade)
  member       users         @relation(fields: [memberId], references: [id], onDelete: Cascade)

  @@id([memberId, conversationId])
  @@index([memberId], type: BTree)
  @@index([conversationId], type: BTree)
}
